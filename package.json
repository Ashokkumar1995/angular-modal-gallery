{
  "name": "angular-modal-gallery",
  "version": "2.0.0-rc.1",
  "description": "Image popup directive for angular >=2",
  "main": "angular-modal-gallery",
  "license": "MIT",
  "author": "Stefano Cappa",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.8.6"
  },
  "homepage": "https://github.com/Ks89/angular-modal-gallery#readme",
  "scripts": {
    "cleanup": "rimraf dist index.ngfactory.ts index.ngsummary.json src/angular-modal-gallery.ngfactory.ts src/angular-modal-gallery.ngsummary.json src/angular-modal-gallery_module.ngfactory.ts src/angular-modal-gallery_module.ngsummary.json",
    "bundling": "rollup -c",
    "minify": "uglifyjs dist/bundles/angular-modal-gallery.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/angular-modal-gallery.umd.min.js",
    "copy": "copyfiles LICENSE README.md dist",
    "copy:packagejson": "copyfiles package.json dist",
    "copy:css": "copyfiles -f styles/style.css dist/styles",
    "copy:dist": "copyfiles -f dist/* demo/webpack/node_modules/angular-modal-gallery",
    "copy:src": "copyfiles -f dist/src/* demo/webpack/node_modules/angular-modal-gallery/src",
    "copy:styles": "copyfiles -f dist/styles/* demo/webpack/node_modules/angular-modal-gallery/styles",
    "copy:bundles": "copyfiles -f dist/bundles/* demo/webpack/node_modules/angular-modal-gallery/bundles",
    "build": "npm run cleanup && ngc && npm run copy:css && npm run copy:packagejson && npm run bundling && npm run minify && npm run copy && npm run copy:dist && npm run copy:src && npm run copy:styles && npm run copy:bundles"
  },
  "dependencies": {
    "@angular/common": "^2.4.4",
    "@angular/compiler": "^2.4.4",
    "@angular/core": "^2.4.4",
    "@angular/platform-browser": "^2.4.4",
    "@angular/platform-browser-dynamic": "^2.4.4",
    "core-js": "^2.4.1",
    "font-awesome": "^4.6.3",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.3",
    "systemjs": "^0.20.2",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/compiler": "^2.4.4",
    "@angular/compiler-cli": "^2.4.4",
    "@angular/platform-server": "^2.4.4",
    "concurrently": "^3.1.0",
    "copyfiles": "^1.0.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.41.4",
    "rollup-plugin-postcss": "^0.2.0",
    "rollup-plugin-sass": "^0.4.8",
    "typescript": "~2.0.10",
    "uglify-js": "^2.7.5"
  },
  "keywords": [
    "angular-modal-gallery",
    "angular2-gsllery",
    "angular2-modal-gallery",
    "angular2-modal",
    "modal-gallery",
    "angular4-modal-gallery",
    "angular4-modal",
    "popup",
    "modal",
    "angular2-modal-popup",
    "angular4-modal-popup",
    "angular2-modal",
    "angular4-modal",
    "modal-popup"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ks89/angular-modal-gallery.git"
  },
  "bugs": {
    "url": "https://github.com/Ks89/angular-modal-gallery/issues"
  }
}
