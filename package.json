{
  "name": "angular-modal-gallery",
  "version": "5.0.0",
  "description": "Modal image gallery for Angular >=4",
  "license": "MIT",
  "author": "Stefano Cappa",
  "homepage": "https://github.com/Ks89",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ks89/angular-modal-gallery.git"
  },
  "engines": {
    "node": ">=9.3.0",
    "npm": ">=5.6.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run build:lib && ng serve",
    "serve": "npm run serve:dev",
    "serve:dev": "ng serve --dev --show-circular-dependencies",
    "serve:prod": "ng serve --prod  --aot --build-optimizer=true",
    "build": "npm run build:lib && npm run build:main",
    "build:lib": "ng-packagr -p libs/angular-modal-gallery/package.json",
    "build:main": "npm run build:main:dev",
    "build:main:dev": "ng build --app=main --dev",
    "build:main:prod": "ng build --app=main --prod --aot --build-optimizer=true",
    "build:examples": "npm run build:lib && npm run clean:examples && npm run copy:dist:examples",
    "build:all": "npm run build:lib && npm run build:main && npm run build:examples",
    "clean": "npm run clean:main",
    "clean:main": "npm run clean:dist && npm run clean:coverage && npm run clean:docs && npm run clean:sonar",
    "clean:dist": "rimraf angular-modal-gallery",
    "clean:coverage": "rimraf coverage",
    "clean:docs": "rimraf docs",
    "clean:sonar": "rimraf reports .scannerwork",
    "clean:systemjs": "rimraf examples/systemjs/node_modules/angular-modal-gallery",
    "clean:webpack": "rimraf examples/webpack/node_modules/angular-modal-gallery examples/webpack/dist examples/webpack/aot examples/webpack/.awcache examples/webpack/coverage",
    "clean:angular-cli": "rimraf examples/angular-cli/node_modules/angular-modal-gallery examples/angular-cli/dist",
    "clean:angular-cli-material": "rimraf examples/angular-cli-material/node_modules/angular-modal-gallery examples/angular-cli-material/dist",
    "clean:universal": "rimraf examples/universal/node_modules/angular-modal-gallery examples/universal/dist",
    "clean:examples": "npm run clean:systemjs && npm run clean:webpack && npm run clean:angular-cli && npm run clean:angular-cli-material && npm run clean:universal",
    "clean:all": "npm run clean:main && npm run clean:examples",
    "copy:dist": "npm run copy:dist:examples",
    "copy:dist:systemjs": "cpr angular-modal-gallery examples/systemjs/node_modules/angular-modal-gallery",
    "copy:dist:webpack": "cpr angular-modal-gallery examples/webpack/node_modules/angular-modal-gallery",
    "copy:dist:angular-cli": "cpr angular-modal-gallery examples/angular-cli/node_modules/angular-modal-gallery",
    "copy:dist:angular-cli-material": "cpr angular-modal-gallery examples/angular-cli-material/node_modules/angular-modal-gallery",
    "copy:dist:universal": "cpr angular-modal-gallery examples/universal/node_modules/angular-modal-gallery",
    "copy:dist:examples": "npm run copy:dist:systemjs && npm run copy:dist:webpack && npm run copy:dist:angular-cli && npm run copy:dist:angular-cli-material && npm run copy:dist:universal",
    "copy:other": "cpr README.md angular-modal-gallery/README.md && cpr LICENSE angular-modal-gallery/LICENSE && cpr CHANGELOG.md angular-modal-gallery/CHANGELOG.md && cpr CONTRIBUTING.md angular-modal-gallery/CONTRIBUTING.md",
    "pretest": "npm run clean:coverage && npm run lint",
    "test": "ng test --watch=false --code-coverage=true",
    "test:snyk": "snyk test && npm run test",
    "test:watch": "ng test --watch=true --code-coverage=true",
    "test:ci": "ng test --watch=false --no-progress --code-coverage=true",
    "test:ci:jenkins": "cross-env-shell CI=true JENKINS=true ng test --watch=false --no-progress --code-coverage=true",
    "e2e": "ng e2e --app=main",
    "e2e:ee": "npm run e2e -- --element-explorer=true",
    "e2e:ci": "npm run e2e -- --no-progress --config=protractor-ci.conf.js",
    "lint": "ng lint",
    "docs": "npm run docs:typedoc && npm run docs:compodoc",
    "docs:typedoc": "typedoc --out ./docs/typedoc --mode file --target ES6 --exclude **/*.spec.ts ./libs/angular-modal-gallery/src",
    "docs:typedoc:serve": "npm run docs:typedoc && lite-server --baseDir=\"docs/typedoc\"",
    "docs:compodoc": "compodoc libs/angular-modal-gallery -p tsconfig.json -d docs/compodoc",
    "docs:compodoc:serve": "npm run docs:compodoc -- -s",
    "docs:compodoc:serve:watch": "npm run docs:compodoc -- -s -w",
    "analyze:sonar": "npm run clean && npm test && sonar-scanner -Dsonar.projectKey=angular-modal-gallery -Dsonar.sources=. -Dsonar.host.url=https://sonarcloud.io -Dsonar.organization=ks89-github",
    "analyze:sonar:ci": "npm run clean && npm run test:ci && sonar-scanner -Dsonar.projectKey=angular-modal-gallery -Dsonar.sources=. -Dsonar.host.url=https://sonarcloud.io -Dsonar.organization=ks89-github",
    "coveralls": "coveralls < coverage/lcov.info",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info",
    "prettier": "prettier --list-different --write \"**/*.*\"",
    "prettier:nowrite": "prettier --list-different \"**/*.*\"",
    "apps:affected": "node ./node_modules/@nrwl/schematics/src/affected/run-affected.js apps",
    "build:affected": "node ./node_modules/@nrwl/schematics/src/affected/run-affected.js build",
    "e2e:affected": "node ./node_modules/@nrwl/schematics/src/affected/run-affected.js e2e",
    "format": "node ./node_modules/prettier/bin/prettier.js --single-quote --print-width 120 --write \"{apps,libs}/**/*.ts\"",
    "nx-migrate": "node ./node_modules/@nrwl/schematics/src/migrator/migrator.js"
  },
  "pre-commit": [
    "prettier"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.1.2",
    "@angular/common": "^5.1.2",
    "@angular/compiler": "^5.1.2",
    "@angular/core": "^5.1.2",
    "@angular/forms": "^5.1.2",
    "@angular/platform-browser": "^5.1.2",
    "@angular/platform-browser-dynamic": "^5.1.2",
    "@angular/router": "^5.1.2",
    "@nrwl/nx": "^0.6.0",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "mousetrap": "^1.6.1",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "file:.angular_cli.tgz",
    "@angular/compiler-cli": "^5.1.2",
    "@angular/language-service": "^5.1.2",
    "@compodoc/compodoc": "^1.0.5",
    "@nrwl/schematics": "^0.6.0",
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "^2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/mousetrap": "^1.6.0",
    "@types/node": "^8.5.2",
    "codeclimate-test-reporter": "^0.5.0",
    "codelyzer": "^4.0.2",
    "coveralls": "^3.0.0",
    "cpr": "^3.0.1",
    "cross-env": "^5.1.3",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.3.1",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.5",
    "karma-safari-launcher": "^1.0.0",
    "karma-sonarqube-unit-reporter": "0.0.14",
    "lite-server": "^2.3.0",
    "ng-packagr": "^1.6.0",
    "pre-commit": "^1.2.2",
    "prettier": "=1.9.2",
    "protractor": "^5.2.2",
    "rimraf": "^2.6.2",
    "sass-lint": "^1.12.1",
    "snyk": "^1.63.0",
    "sonar-scanner": "^3.0.3",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "typedoc": "^0.9.0",
    "typescript": "=2.5.3"
  },
  "browserslist": [
    "last 2 versions",
    "not ie <= 10",
    "not ie_mob <= 10"
  ]
}
