{
  "name": "angular-modal-gallery",
  "version": "5.0.0",
  "description": "Modal image gallery for Angular >=4",
  "main": "src/main.ts",
  "license": "MIT",
  "author": "Stefano Cappa",
  "homepage": "https://github.com/Ks89",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ks89/angular-modal-gallery.git"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run build:lib && ng serve",
    "build": "npm run build:lib && ng build",
    "build:dev": "ng build --app dev",
    "build:prod": "ng build --prod",
    "build:aot": "ng build --aot",
    "build:app": "npm run build:lib && ng build --app packages",
    "build:lib": "npm run clean && ng-packagr && npm run copy:other && npm run copy:dist",
    "build:examples": "npm run clean:examples && npm run copy:dist:examples",
    "build:all": "npm run build:lib && npm run build:examples",
    "clean": "npm run clean:main",
    "clean:main": "npm run clean:dist && npm run clean:coverage && npm run clean:docs",
    "clean:dist": "rimraf dist src/angular-modal-gallery",
    "clean:coverage": "rimraf coverage",
    "clean:docs": "rimraf docs documentation",
    "clean:systemjs": "rimraf examples/systemjs/node_modules/angular-modal-gallery",
    "clean:webpack": "rimraf examples/webpack/node_modules/angular-modal-gallery examples/webpack/dist examples/webpack/aot examples/webpack/.awcache examples/webpack/coverage",
    "clean:angular-cli": "rimraf examples/angular-cli/node_modules/angular-modal-gallery examples/angular-cli/dist",
    "clean:universal": "rimraf examples/universal/node_modules/angular-modal-gallery examples/universal/dist",
    "clean:examples": "npm run clean:systemjs && npm run clean:webpack && npm run clean:angular-cli && npm run clean:universal",
    "clean:all": "npm run clean:main && npm run clean:examples",
    "copy:dist": "npm run copy:dist:main",
    "copy:dist:main": "cpr dist/angular-modal-gallery src/angular-modal-gallery",
    "copy:dist:systemjs": "cpr dist/angular-modal-gallery examples/systemjs/node_modules/angular-modal-gallery",
    "copy:dist:webpack": "cpr dist/angular-modal-gallery examples/webpack/node_modules/angular-modal-gallery",
    "copy:dist:angular-cli": "cpr dist/angular-modal-gallery examples/angular-cli/node_modules/angular-modal-gallery",
    "copy:dist:universal": "cpr dist/angular-modal-gallery examples/universal/node_modules/angular-modal-gallery",
    "copy:dist:examples": "npm run copy:dist:systemjs && npm run copy:dist:webpack && npm run copy:dist:angular-cli && npm run copy:dist:universal",
    "copy:dist:all": "npm run copy:dist:main && npm run copy:dist:examples",
    "copy:other": "cpr README.md dist/angular-modal-gallery/README.md && cpr LICENSE dist/angular-modal-gallery/LICENSE && cpr CHANGELOG.md dist/angular-modal-gallery/CHANGELOG.md && cpr CONTRIBUTING.md dist/angular-modal-gallery/CONTRIBUTING.md",
    "pretest": "npm run clean:coverage",
    "test": "karma start --single-run",
    "test:watch": "karma start",
    "docs": "typedoc --out docs angular-modal-gallery/src",
    "compodoc": "compodoc -p ./tsconfig.compodoc.json",
    "coveralls": "coveralls < coverage/lcov.info",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "dependencies": {
    "@angular/animations": "^5.1.0",
    "@angular/common": "^5.1.0",
    "@angular/compiler": "^5.1.0",
    "@angular/core": "^5.1.0",
    "@angular/forms": "^5.1.0",
    "@angular/http": "^5.1.0",
    "@angular/platform-browser": "^5.1.0",
    "@angular/platform-browser-dynamic": "^5.1.0",
    "@angular/router": "^5.1.0",
    "core-js": "^2.5.1",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "mousetrap": "^1.6.1",
    "rxjs": "^5.5.5",
    "sass-lint": "^1.12.1",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.0",
    "@angular/compiler-cli": "^5.1.0",
    "@angular/language-service": "^5.1.0",
    "@compodoc/compodoc": "^1.0.5",
    "@types/jasmine": "^2.8.2",
    "@types/mousetrap": "^1.5.33",
    "@types/node": "^8.0.56",
    "codeclimate-test-reporter": "^0.5.0",
    "codelyzer": "^3.2.2",
    "coveralls": "^3.0.0",
    "cpr": "^3.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^1.6.0",
    "protractor": "^5.2.1",
    "rimraf": "^2.6.1",
    "ts-node": "~3.3.0",
    "tslint": "^5.8.0",
    "typedoc": "^0.9.0",
    "typescript": "=2.5.3"
  }
}
